{
  "name": "server",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm-run-all --parallel dev:watch playground",
    "dev:watch": "cross-env NODE_ENV=development nodemon -e ts,graphql -x ts-node src/index.ts",
    "gen:types": "gql-gen",
    "playground": "graphql playground",
    "start": "ts-node src/index.ts",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:gui": "majestic",
    "data:seed": "cross-env NODE_ENV=development ts-node src/utils/seedData/index.ts",
    "data:nuke": "cross-env NODE_ENV=development NUKE_DB=true ts-node src/utils/seedData/nukeData.ts"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "^0.0.8",
    "@types/express-rate-limit": "^3.3.0",
    "@types/express-session": "^1.15.11",
    "@types/faker": "^4.1.5",
    "@types/glob": "^7.1.1",
    "@types/ioredis": "^4.0.6",
    "@types/jest": "^23.3.11",
    "@types/node": "^10.12.18",
    "@types/node-fetch": "^2.1.4",
    "@types/nodemailer": "^4.6.5",
    "@types/passport": "^1.0.0",
    "@types/passport-twitter": "^1.0.34",
    "@types/request-promise": "^4.1.42",
    "@types/uuid": "^3.4.4",
    "@types/yup": "^0.26.4",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.2.0",
    "graphql-cli": "^3.0.3",
    "graphql-code-generator": "^0.15.2",
    "graphql-codegen-typescript-common": "^0.15.2",
    "graphql-codegen-typescript-resolvers": "^0.15.2",
    "graphql-codegen-typescript-server": "^0.15.2",
    "graphql-request": "^1.8.2",
    "jest": "^23.6.0",
    "majestic": "^1.2.23",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^23.10.5",
    "ts-node": "7.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-redis": "^3.4.0",
    "dotenv": "^6.2.0",
