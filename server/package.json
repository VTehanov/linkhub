    "merge-graphql-schemas": "^1.5.8",
    "node-fetch": "^2.3.0",
    "nodemailer": "^5.1.1",
    "passport": "^0.4.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.3.0",
    "rate-limit-redis": "^1.6.0",
    "reflect-metadata": "^0.1.10",
    "request-promise": "^4.2.2",
    "slugify": "^1.3.4",
    "typeorm": "0.2.9",
    "uuid": "^3.3.2",
    "yup": "^0.26.6"
  }
